local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local LocalPlayer = Players.LocalPlayer

-- Remote
local CastEvent = LocalPlayer.Character:WaitForChild("Champions"):WaitForChild("Mechanics"):WaitForChild("Remotes"):WaitForChild("CastEvent")
local MiniGame = LocalPlayer.Character:WaitForChild("Champions"):WaitForChild("Mechanics"):WaitForChild("Remotes"):WaitForChild("MiniGame")
local RequestFishAnnouncement = ReplicatedStorage:WaitForChild("RequestFishAnnouncement")
local GetCaughtFishInfo = ReplicatedStorage:WaitForChild("GetCaughtFishInfo")

-- Fish list
local fishList = {"Boneka Biru", "Boneka Merah", "Boneka Hijau", "Boneka Kuning", "Boneka Ungu"}

-- Main loop
print("ðŸŽ£ Auto Fishing Started (7s delay)")

while true do
    -- Cast
    pcall(CastEvent.FireServer, CastEvent, false, 100)
    wait(1)
    
    -- Mini game
    pcall(MiniGame.FireServer, MiniGame, true)
    
    -- Random fish
    local fish = fishList[math.random(#fishList)]
    
    -- Announce
    pcall(function()
        local doll = LocalPlayer.Backpack:FindFirstChild(fish .. " (920.0 kg)")
        if doll then
            RequestFishAnnouncement:FireServer(doll)
        end
    end)
    
    -- Get info
    pcall(GetCaughtFishInfo.InvokeServer, GetCaughtFishInfo, fish)
    
    -- Wait 7 seconds
    wait(7)
end
