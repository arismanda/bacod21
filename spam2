        CleanButton = CleanButton
    }
end

-- ==================== STATS UPDATER ====================
local function updateStatsDisplay(statusLabel)
    local lastUpdate = tick()
    local lastCount = Stats.TotalRequests
    
    while true do
        local now = tick()
        local delta = now - lastUpdate
        
        if delta >= 1 then
            Stats.CurrentRPS = (Stats.TotalRequests - lastCount) / delta
            lastUpdate = now
            lastCount = Stats.TotalRequests
            
            -- Update GUI
            statusLabel.Text = string.format(
                "STATUS: %s\nRPS: %d\nTOTAL: %d\nTHREADS: %d\nMEMORY: COLLECTING...",
                AttackActive and "ATTACKING" or "IDLE",
                Stats.CurrentRPS,
                Stats.TotalRequests,
                #Threads
            )
        end
        
        task.wait(0.1)
    end
end

-- ==================== MAIN EXECUTION ====================
local function initializeGUI()
    -- Initialize all tab content
    local floodUI = createRemoteFlooderUI(tabFrames["FLOOD"])
    local controlUI = createControlPanel(tabFrames["CONTROL"])
    
    -- Setup content frames
    for _, frame in pairs(tabFrames) do
        frame.CanvasSize = UDim2.new(0, 0, 0, 400)
    end
    
    -- Stats updater
    task.spawn(function()
        updateStatsDisplay(controlUI.StatusLabel)
    end)
    
    -- Control bindings
    controlUI.StartButton.MouseButton1Click:Connect(function()
        if not AttackActive then
            AttackActive = true
            
            -- Start all attack modules
            Threads = {}
            
            table.insert(Threads, task.spawn(executeNetworkOverload))
            table.insert(Threads, task.spawn(function()
                while AttackActive do
                    executeMemoryOverload(5)
                    task.wait(1)
                end
            end))
            
            controlUI.StartButton.BackgroundColor3 = Color3.fromRGB(50, 200, 50)
            controlUI.StartButton.Text = "âš¡ ATTACKING..."
        end
    end)
    
    controlUI.StopButton.MouseButton1Click:Connect(function()
        AttackActive = false
        
        -- Kill all threads
        for _, thread in pairs(Threads) do
            pcall(task.cancel, thread)
        end
        Threads = {}
        
        controlUI.StartButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
        controlUI.StartButton.Text = "âš¡ START ALL"
    end)
    
    controlUI.CleanButton.MouseButton1Click:Connect(function()
        -- Clean up created objects
        for _, obj in pairs(workspace:GetChildren()) do
            if obj.Name == "Part" and obj:IsA("Part") then
                obj:Destroy()
            end
        end
        
        collectgarbage("collect")
        Stats.TotalRequests = 0
    end)
    
    floodUI.FloodButton.MouseButton1Click:Connect(function()
        if not AttackActive then
            AttackActive = true
            
            local targetRPS = tonumber(floodUI.RPSSlider.Text) or 10000
            print("[OVERLOAD] Starting flood with RPS:", targetRPS)
            
            floodUI.FloodButton.BackgroundColor3 = Color3.fromRGB(50, 200, 50)
            floodUI.FloodButton.Text = "FLOODING..."
        else
            AttackActive = false
            floodUI.FloodButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
            floodUI.FloodButton.Text = "ðŸš€ START FLOOD"
        end
    end)
    
    -- Close button
    local CloseButton = Instance.new("TextButton")
    CloseButton.Parent = MainFrame
    CloseButton.Position = UDim2.new(1, -25, 0, 5)
    CloseButton.Size = UDim2.new(0, 20, 0, 20)
    CloseButton.Text = "X"
    CloseButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
    CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    CloseButton.MouseButton1Click:Connect(function()
        ScreenGui:Destroy()
        AttackActive = false
    end)
    
    -- Minimize button
    local MinButton = Instance.new("TextButton")
    MinButton.Parent = MainFrame
    MinButton.Position = UDim2.new(1, -50, 0, 5)
    MinButton.Size = UDim2.new(0, 20, 0, 20)
    MinButton.Text = "_"
    MinButton.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
    MinButton.MouseButton1Click:Connect(function()
        MainFrame.Visible = not MainFrame.Visible
    end)
end

-- ==================== ANTI-DETECTION ====================
local function applyAntiDetection()
    -- Randomize GUI position
    MainFrame.Position = UDim2.new(
        math.random(0.1, 0.7),
        0,
        math.random(0.1, 0.7),
        0
    )
    
    -- Change GUI color periodically
    task.spawn(function()
        while true do
            local hue = tick() % 1
            Title.BackgroundColor3 = Color3.fromHSV(hue, 0.7, 0.3)
            task.wait(2)
        end
    end)
    
    -- Encrypt console output
    local oldPrint = print
    print = function(...)
        local args = {...}
        local encrypted = ""
        for _, arg in pairs(args) do
            encrypted = encrypted .. tostring(arg):gsub(".", function(c)
                return string.char(c:byte() + 1)
            end) .. " "
        end
        oldPrint("[ENCRYPTED]", encrypted)
    end
end

-- ==================== INITIALIZATION ====================
-- Wait for game to load
task.wait(2)

-- Initialize
initializeGUI()
applyAntiDetection()

print("[DYRON OVERLOAD v3.0] GUI Initialized")
print("[WARNING] This tool is for educational purposes only")
print("[INFO] Use at your own risk - High ban probability")

-- Auto-cleanup on player leave
game:GetService("Players").PlayerRemoving:Connect(function(player)
    if player == localPlayer then
        AttackActive = false
        ScreenGui:Destroy()
    end
end)
