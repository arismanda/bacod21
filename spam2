    StopButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    
    yOffset += 40
    
    -- Cleanup Button
    local CleanButton = Instance.new("TextButton")
    CleanButton.Parent = parentFrame
    CleanButton.Position = UDim2.new(0.1, 0, 0, yOffset)
    CleanButton.Size = UDim2.new(0.8, 0, 0, 25)
    CleanButton.Text = "ðŸ§¹ CLEAN UP"
